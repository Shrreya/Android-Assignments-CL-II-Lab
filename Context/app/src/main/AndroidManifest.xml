<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shrreya.context" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.shrreya.context.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.shrreya.context.DataEntryActivity"
            android:label="@string/title_activity_data_entry"
            android:parentActivityName="com.example.shrreya.context.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.shrreya.context.MainActivity" />
        </activity>
        <activity
            android:name="com.example.shrreya.context.CompanyContextActivity"
            android:label="@string/title_activity_company_context"
            android:parentActivityName="com.example.shrreya.context.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.shrreya.context.MainActivity" />
        </activity>
        <activity
            android:name="com.example.shrreya.context.MatrimonyContextActivity"
            android:label="@string/title_activity_matrimony_context"
            android:parentActivityName="com.example.shrreya.context.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.shrreya.context.MainActivity" />
        </activity>
    </application>

</manifest>
